    <h1>Janelia FlyLight Larval Raw Images</h1>
    
    <div class="gallery">
    <% thumbnails.forEach(thumb => { %>
        <div class="thumbnail">
            <a href="/images/<%= thumb.dir %>.tar.gz"><%= thumb.dir.replace('lmbjanupload-','').replace('-Y','') %></a><br>
            <a href="/images/<%= thumb.dir %>/<%= thumb.name.replace('_G', '').replace('.jpg', '.tif') %>"><img src="<%= thumb.path %>" alt="<%= thumb.name %>">
            <br><%= thumb.name.replace('.jpg', '').replace('_G', '') %></a>
        </div>
    <% }); %>
    </div>
